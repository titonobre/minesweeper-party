{
  "name": "minesweeper-party",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-server": "node-dev server",
    "build": "npm-run-all build:*",
    "build:app": "ng build --prod --aot --build-optimizer --stats-json --source-map --extract-licenses",
    "build:server": "tsc -p server --outDir dist/server",
    "postinstall": "ngcc",
    "prebuild": "npm-run-all prepare:*",
    "prepare:generate-app-image": "svgexport svgexport.json",
    "prepare:minify-app-image": "imagemin src/assets/icons/*.png --out-dir=src/assets/icons",
    "serve": "node dist/server/server/server.js",
    "lint": "ng lint"
  },
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "dependencies": {
    "@angular/animations": "11.2.0",
    "@angular/cdk": "11.2.0",
    "@angular/common": "11.2.0",
    "@angular/compiler": "11.2.0",
    "@angular/core": "11.2.0",
    "@angular/forms": "11.2.0",
    "@angular/material": "11.2.0",
    "@angular/platform-browser": "11.2.0",
    "@angular/platform-browser-dynamic": "11.2.0",
    "@angular/router": "11.2.0",
    "@angular/service-worker": "11.2.0",
    "compression": "1.7.4",
    "core-js": "^3.8.2",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.3",
    "pusher": "3.0.1",
    "pusher-js": "6.0.3",
    "rxjs": "6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.3",
    "@angular/cli": "11.1.4",
    "@angular/compiler-cli": "11.1.2",
    "@angular/language-service": "11.1.2",
    "@types/node": "14.14.25",
    "codelyzer": "^6.0.0",
    "imagemin": "7.0.1",
    "imagemin-cli": "6.0.0",
    "node-dev": "6.2.0",
    "npm-run-all": "4.1.5",
    "svgexport": "0.4.1",
    "ts-node": "9.1.1",
    "tslint": "~6.1.0",
    "typescript": "4.0.5"
  },
  "heroku-run-build-script": true
}
